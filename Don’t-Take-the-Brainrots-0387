-- Redz Hub for Don't Take the Brainrots
-- Full version with multiple features

local redzlib = loadstring(game:HttpGet("https://raw.githubusercontent.com/tbao143/Library-ui/refs/heads/main/Redzhubui"))()

-- Create the main window
local Window = redzlib:MakeWindow({
    Title = "Brainrots Hub",
    SubTitle = "by danno | v2.0",
    SaveFolder = "BrainrotsHubConfig"
})

-- Add a minimize button
Window:AddMinimizeButton({
    Button = {
        Image = "rbxassetid://125003246208510",
        BackgroundTransparency = 0
    },
    Corner = {
        CornerRadius = UDim.new(0, 5)
    }
})

-- First tab: Discord invite
local DiscordInviteTab = Window:MakeTab({"Discord", "mail"})

DiscordInviteTab:AddDiscordInvite({
    Name = "Support Discord",
    Description = "Join for updates, support and scripts",
    Logo = "rbxassetid://125003246208510",
    Invite = "https://discord.gg/97NybfMde"
})

-- Main Tab
local MainTab = Window:MakeTab({"Main", "home"})

-- Auto-Farm Section
MainTab:AddSection("Auto-Farm")

MainTab:AddToggle({
    Name = "Auto Collect Brains",
    Default = false,
    Callback = function(Value)
        _G.AutoCollectBrains = Value
        if Value then
            spawn(function()
                while _G.AutoCollectBrains do
                    wait(0.1)
                    -- Auto collect brain function
                    pcall(function()
                        local brains = game:GetService("Workspace"):FindFirstChild("Brains")
                        if brains then
                            for _, brain in pairs(brains:GetChildren()) do
                                if brain:IsA("BasePart") then
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = brain.CFrame
                                    wait(0.05)
                                end
                            end
                        end
                    end)
                end
            end)
        end
    end
})

MainTab:AddToggle({
    Name = "Auto Complete Quests",
    Default = false,
    Callback = function(Value)
        _G.AutoQuests = Value
        if Value then
            spawn(function()
                while _G.AutoQuests do
                    wait(1)
                    pcall(function()
                        -- Auto quest completion logic
                        local quests = game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("Quests")
                        if quests then
                            -- Add quest completion code here
                        end
                    end)
                end
            end)
        end
    end
})

MainTab:AddToggle({
    Name = "Auto Rebirth",
    Default = false,
    Callback = function(Value)
        _G.AutoRebirth = Value
        if Value then
            spawn(function()
                while _G.AutoRebirth do
                    wait(5)
                    pcall(function()
                        -- Rebirth function
                        local rebirthRemote = game:GetService("ReplicatedStorage"):FindFirstChild("Rebirth")
                        if rebirthRemote then
                            rebirthRemote:FireServer()
                        end
                    end)
                end
            end)
        end
    end
})

-- Combat Section
MainTab:AddSection("Combat")

MainTab:AddToggle({
    Name = "Auto Attack Enemies",
    Default = false,
    Callback = function(Value)
        _G.AutoAttack = Value
        if Value then
            spawn(function()
                while _G.AutoAttack do
                    wait(0.3)
                    pcall(function()
                        local enemies = game:GetService("Workspace"):FindFirstChild("Enemies")
                        if enemies then
                            for _, enemy in pairs(enemies:GetChildren()) do
                                if enemy:FindFirstChild("HumanoidRootPart") then
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = enemy.HumanoidRootPart.CFrame
                                    -- Attack remote here
                                    wait(0.2)
                                end
                            end
                        end
                    end)
                end
            end)
        end
    end
})

MainTab:AddToggle({
    Name = "Kill Aura",
    Default = false,
    Callback = function(Value)
        _G.KillAura = Value
        if Value then
            spawn(function()
                while _G.KillAura do
                    wait(0.2)
                    pcall(function()
                        local character = game.Players.LocalPlayer.Character
                        if character then
                            local range = 20
                            local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
                            if humanoidRootPart then
                                for _, player in pairs(game.Players:GetPlayers()) do
                                    if player ~= game.Players.LocalPlayer then
                                        local targetChar = player.Character
                                        if targetChar and targetChar:FindFirstChild("HumanoidRootPart") then
                                            local distance = (humanoidRootPart.Position - targetChar.HumanoidRootPart.Position).Magnitude
                                            if distance <= range then
                                                -- Attack nearby players
                                            end
                                        end
                                    end
                                end
                            end
                        end
                    end)
                end
            end)
        end
    end
})

-- Teleport Section
MainTab:AddSection("Teleports")

local locations = {
    {"Spawn Area", CFrame.new(0, 10, 0)},
    {"Brain Farm", CFrame.new(50, 10, 50)},
    {"Enemy Area", CFrame.new(100, 10, 0)},
    {"Secret Zone", CFrame.new(-100, 10, -50)}
}

for _, location in pairs(locations) do
    MainTab:AddButton({
        Name = "TP to " .. location[1],
        Callback = function()
            pcall(function()
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = location[2]
            end)
        end
    })
end

